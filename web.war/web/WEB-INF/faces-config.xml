<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">
	<managed-bean>
	<managed-bean-name>visualStatusIndicatorBean</managed-bean-name>
	<managed-bean-class>com.varian.production.web.podplugin.VisualStatusIndicatorPlugin</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
	<managed-property>
			<property-name>tableConfigBean</property-name>
			<value>#{sfcStatusTableBeanConfigurator}</value>
		</managed-property>
	</managed-bean> 
	<managed-bean>
		<managed-bean-name>sfcStatusTableBeanConfigurator</managed-bean-name>
		<managed-bean-class>com.sap.me.wpmf.TableConfigurator</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>sfcStatusTableBeanConfigurator</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
        <managed-bean-name>logTqcBuyoffBean</managed-bean-name>
        <managed-bean-class>com.varian.production.web.podplugin.LogTqcBuyoffPlugin</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
			<property-name>tableConfig</property-name>
			<value>#{tqcBuyoffTableBeanConfigurator}</value>
		</managed-property>
    </managed-bean>
    <managed-bean>
		<managed-bean-name>tqcBuyoffTableBeanConfigurator</managed-bean-name>
		<managed-bean-class>com.sap.me.production.podclient.PodClientTableConfigurator</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>tqcBuyoffTableBeanConfigurator</value>
		</managed-property>
				<managed-property>
			<property-name>columnBindings</property-name>
			<property-class>java.util.HashMap</property-class>
			<map-entries>
				<key-class>java.lang.String</key-class>
				<value-class>java.lang.String</value-class>
				<map-entry>
					<key>SFC</key>
					<value>sfc;sfc.plugin.default.LABEL</value>
				</map-entry>
				<map-entry>
					<key>BUYOFF</key>
					<value>buyoff;buyoff.plugin.default.LABEL</value>
				</map-entry>
				<map-entry>
					<key>DESCRIPTION</key>
					<value>description;description.plugin.default.LABEL </value>
				</map-entry>
				<map-entry>
					<key>ACTION</key>
					<value>action;action.plugin.default.LABEL </value>
				</map-entry>
				<map-entry>
					<key>COMMENTS</key>
					<value>comments;comments.plugin.default.LABEL </value>
				</map-entry>
				<map-entry>
					<key>USER</key>
					<value>pastUser;pastUser.plugin.default.LABEL </value>
				</map-entry>
				<map-entry>
					<key>STATE</key>
					<value>buyoffState;buyoffState.plugin.default.LABEL </value>
				</map-entry>
			</map-entries>
		</managed-property>
		<managed-property>
			<property-name>columnWidths</property-name>
			<property-class>java.util.HashMap</property-class>
			<map-entries>
				<key-class>java.lang.String</key-class>
				<value-class>java.lang.String</value-class>
				<map-entry>
					<key>SFC</key>
					<value>10em</value>
				</map-entry>
				<map-entry>
					<key>BUYOFF</key>
					<value>10em</value>
				</map-entry>
				<map-entry>
					<key>DESCRIPTION</key>
					<value>13em</value>
				</map-entry>
				<map-entry>
					<key>ACTION</key>
					<value>10em</value>
				</map-entry>
				<map-entry>
					<key>COMMENTS</key>
					<value>10em</value>
				</map-entry>
				<map-entry>
					<key>USER</key>
					<value>10em</value>
				</map-entry>
				<map-entry>
					<key>STATE</key>
					<value>10em</value>
				</map-entry>
			</map-entries>
		</managed-property>
	</managed-bean>
	<!-- Defect 450  -->
    <managed-bean>
		<managed-bean-name>toolLogBean</managed-bean-name>
	<managed-bean-class>com.varian.lsf.tool.ToolLoggingScreenController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>tableConfigBean</property-name>
			<value>#{toolLogBeanConfigurator}</value>
		</managed-property>
		<managed-property>
			<property-name>tabletoolGroupBean</property-name>
			<value>#{toolGroupBeanConfigurator}</value>
		</managed-property>
		<managed-property>
			<property-name>tabletoolNumberBean</property-name>
			<value>#{toolNumberBeanConfigurator}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>toolLogBeanConfigurator</managed-bean-name>
		<managed-bean-class>com.sap.me.wpmf.TableConfigurator</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>toolLogBeanConfigurator</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>toolGroupBeanConfigurator</managed-bean-name>
		<managed-bean-class>com.sap.me.wpmf.TableConfigurator</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>toolGroupBeanConfigurator</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>toolNumberBeanConfigurator</managed-bean-name>
		<managed-bean-class>com.sap.me.wpmf.TableConfigurator</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>toolNumberBeanConfigurator</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>operationBrowseBean</managed-bean-name>
		<managed-bean-class>com.varian.lsf.tool.OperationBrowse</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>configBean</property-name>
			<value>#{operationTableBeanConfigurator}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>operationTableBeanConfigurator</managed-bean-name>
		<managed-bean-class>com.sap.me.wpmf.TableConfigurator</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>operationTableBeanConfigurator</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
        <managed-bean-name>logToolBean</managed-bean-name>
        <managed-bean-class>com.varian.production.web.podplugin.LogToolPlugin</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
			<property-name>tableConfigBean</property-name>
			<value>#{logtoolSfcBeanConfigurator}</value>
		</managed-property>
		<managed-property>
			<property-name>tabletoolGroupBean</property-name>
			<value>#{logtoolGroupBeanConfigurator}</value>
		</managed-property>
		<managed-property>
			<property-name>tabletoolNumberBean</property-name>
			<value>#{logtoolNumberBeanConfigurator}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>logtoolSfcBeanConfigurator</managed-bean-name>
		<managed-bean-class>com.sap.me.wpmf.TableConfigurator</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>logtoolSfcBeanConfigurator</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>logtoolGroupBeanConfigurator</managed-bean-name>
		<managed-bean-class>com.sap.me.wpmf.TableConfigurator</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>logtoolGroupBeanConfigurator</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>logtoolNumberBeanConfigurator</managed-bean-name>
		<managed-bean-class>com.sap.me.wpmf.TableConfigurator</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>logtoolNumberBeanConfigurator</value>
		</managed-property>
	</managed-bean>
</faces-config>
