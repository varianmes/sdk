<?xml version="1.0" encoding="UTF-8" standalone="no"?><SQLQuery AllowBuffering="false" AllowFuture="true" CacheDuration="0" CacheDurationUnits="M" Columns="" Comment="" Connector="" DateColumn="" DateFormat="MM/dd/yyyy HH:mm:ss" Debug="false" Description="" DocType="SQLQuery" Duration="60" DurationUnits="M" EndDate="" FilterExpr="" Group="" GroupingExpr="" ID="11393" InlineTransform="server://XMII/CM/VMSME/Reports/ReceptorHistory/ReceptorHistory.xsl" IntervalCount="1" IsCachable="false" JoinExpr="" Mask="" Method="" Mode="FixedQuery" NumberFormat="0.00" Param.1="" Param.10="" Param.11="" Param.12="" Param.13="" Param.14="" Param.15="" Param.16="" Param.17="" Param.18="" Param.19="" Param.2="" Param.20="" Param.21="" Param.22="" Param.23="" Param.24="" Param.25="" Param.26="" Param.27="" Param.28="" Param.29="" Param.3="" Param.30="" Param.31="" Param.32="" Param.4="" Param.5="" Param.6="" Param.7="" Param.8="" Param.9="" ParamType.1="" ParamType.10="" ParamType.11="" ParamType.12="" ParamType.13="" ParamType.14="" ParamType.15="" ParamType.16="" ParamType.17="" ParamType.18="" ParamType.19="" ParamType.2="" ParamType.20="" ParamType.21="" ParamType.22="" ParamType.23="" ParamType.24="" ParamType.25="" ParamType.26="" ParamType.27="" ParamType.28="" ParamType.29="" ParamType.3="" ParamType.30="" ParamType.31="" ParamType.32="" ParamType.4="" ParamType.5="" ParamType.6="" ParamType.7="" ParamType.8="" ParamType.9="" Password="" PathID="1064" Query="declare @serial as varchar(20)&#10;select @serial =  '[Param.1]'&#10;(SELECT A.SFC,A.ITEM AS MATERIAL,CAST(b.Receptor_Type AS VARCHAR(20)) AS RECEPTOR_TYPE,CAST(B.CUSTOMER AS VARCHAR(20)) AS CUSTOMER,A.OPERATION,A.RESRCE AS RESOURCE,A.ACTION_CODE AS ACTIVITY_CODE,&#10;CAST(' ' AS NVARCHAR(2048)) AS COMMENTS,CAST(' ' AS VARCHAR(20)) AS NC_STATE,CAST(' ' AS VARCHAR(20)) AS REWORK_STATE,A.DATE_TIME,A.USER_ID&#10;FROM ACTIVITY_LOG A&#10;JOIN dbo.VMS_MAT_CUST_RECEPTORTYPE B ON   A.ITEM = B.ITEM&#10;WHERE A.SITE='0536' AND &#10;A.SFC in (select SFC from SFC where HANDLE in (select SFC_BO from SFC_DATA where sfc_data.DATA_ATTR = @serial ) ) AND&#10;A.ACTION_CODE = 'COMPLETE' AND A.ACTIVITY = 'PR510')&#10;&#10;UNION ALL&#10;&#10;&#10;&#10;(SELECT B.SFC,C.ITEM AS MATERIAL,CAST(H.RECEPTOR_TYPE AS VARCHAR(20)) AS RECEPTOR_TYPE,CAST(H.CUSTOMER AS VARCHAR(20)) AS CUSTOMER,E.OPERATION,D.RESRCE AS RESOURCE,F.NC_CODE AS ACTIVITY_CODE,CAST(A.COMMENTS AS NVARCHAR(2048)) AS COMMENTS,CAST(A.NC_STATE AS VARCHAR(20)) AS NC_STATE,&#10;REWORK_STATE = CASE FAILURE_ID WHEN  'PASS' THEN 'PASS' WHEN 'FAIL' THEN 'FAIL' ELSE '-' END,a.DATE_TIME,G.USER_ID FROM NC_DATA A&#10;JOIN SFC B ON A.NC_CONTEXT_GBO = B.HANDLE&#10;JOIN ITEM C ON A.ITEM_BO = C.HANDLE&#10;JOIN RESRCE D ON A.RESOURCE_BO = D.HANDLE&#10;JOIN OPERATION E ON A.OPERATION_BO = E.HANDLE&#10;JOIN NC_CODE F ON A.NC_CODE_BO = F.HANDLE&#10;JOIN USR G on a.user_BO = g.handle&#10;JOIN dbo.VMS_MAT_CUST_RECEPTORTYPE H ON   c.ITEM = H.ITEM&#10;WHERE A.NC_CONTEXT_GBO IN (select sfc_data.SFC_BO from SFC_DATA where sfc_data.DATA_ATTR = @serial ) AND &#10;A.NC_STATE !='D' AND F.NC_CODE != 'LOG_REWORK_STATE')&#10;&#10;UNION ALL&#10;&#10;&#10;&#10;(select A.SFC,A.ITEM AS MATERIAL,CAST(C.RECEPTOR_TYPE AS VARCHAR(20))AS RECEPTOR_TYPE,CAST(C.CUSTOMER AS VARCHAR(20))AS CUSTOMER,A.OPERATION,A.RESRCE AS RESOURCE,A.ACTION_CODE as ACTIVITY_CODE,&#10;CAST(B.COMMENTS AS NVARCHAR(2048)) AS COMMENTS,CAST(' ' AS VARCHAR(20)) AS NC_STATE,CAST(' ' AS VARCHAR(20)) AS REWORK_STATE,&#10;A.DATE_TIME as DATE_TIME,A.USER_ID from ACTIVITY_LOG A&#10;inner join PRODUCTION_COMMENT B on&#10;B.CREATED_DATE_TIME = A.DATE_TIME&#10;JOIN dbo.VMS_MAT_CUST_RECEPTORTYPE C ON   A.ITEM = C.ITEM&#10;where A.SFC in (select SFC from SFC where HANDLE in (select SFC_BO from SFC_DATA where sfc_data.DATA_ATTR = @serial ) )&#10;and A.ACTIVITY = 'LOG_COMMENT'&#10;and A.CHANGE_STAMP is null)&#10;&#10;ORDER BY DATE_TIME&#10;" QueryParams="" ReaderRoles="SAP_XMII_User" RestrictedPropertyOverride="false" RowCount="1